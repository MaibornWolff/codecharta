{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "visualization": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "app",
      "prefix": "cc",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/webpack",
            "index": "app/index.html",
            "main": "app/main.ts",
            "polyfills": "app/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "sass",
            "assets": ["app/codeCharta/assets"],
            "styles": ["app/app.scss", "material-icons/iconfont/material-icons.css"],
            "stylePreprocessorOptions": {
              "includePaths": ["app/material"]
            },
            "customWebpackConfig": {
              "path": "conf/webpack.config.js"
            },
            "scripts": [],
            "allowedCommonJsDependencies": ["ajv", "color-convert", "html2canvas", "ignore", "md5", "three-orbit-controls"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "algo-dev": {
              "fileReplacements": [
                {
                  "replace": "app/main.ts",
                  "with": "app/codeCharta/util/algorithm/2DTestEnvironment/main.algo.ts"
                }
              ],
              "index": "app/codeCharta/util/algorithm/2DTestEnvironment/index.algo.html",
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "visualization:build:production"
            },
            "development": {
              "buildTarget": "visualization:build:development",
              "port": 3000,
              "open": true
            },
            "algo-dev": {
              "buildTarget": "visualization:build:algo-dev",
              "port": 4200,
              "open": true
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
