{

    "type": "object",

    "required":["projectName", "nodes"],

    "properties": {
        "projectName": {
            "type": "string"
        },
        "nodes": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/node"
            }
        }
    },

    "definitions": {
        "node":{
            "type": "object",
            "required":["name"],
            "properties": {
                "name": {
                    "type": "string"
                },
                "children": {
                    "type": "array",
                    "minItems": 0,
                    "items": {
                        "$ref": "#/definitions/node"
                    }
                },
                "attributes": {
                    "$ref": "#/definitions/attributeList"
                }
            }
        },
        "attributeList":{
            "type":"object",
            "patternProperties": {
                "^.*$":{
                    "type": "number"
                }
            }
        }
    }
}