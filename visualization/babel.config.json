{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				},
				"corejs": 3,
				"useBuiltIns": "usage"
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": ["angularjs-annotate"],
	"env": {
		"test": {
			"plugins": ["transform-es2015-modules-commonjs"]
		},
		"development": {
			"plugins": ["istanbul"]
		}
	}
}
