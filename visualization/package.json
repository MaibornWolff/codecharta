		"@angular/forms": "^12.2.12",
		"@angular/material": "^12.2.12",
		"@angular/platform-browser": "^12.2.12",
		"@angular/platform-browser-dynamic": "^12.2.12",
		"@types/webpack": "^5.28.0",
		"ajv": "^6.12.6",
		"canvas": "^2.10.2",
		"color-convert": "^2.0.1",
		"core-js": "^3.15.2",
		"d3-hierarchy": "^2.0.0",
		"font-awesome": "^4.7.0",
		"hotkeys-js": "^3.8.2",
		"ignore": "^5.1.8",
		"klona": "^2.0.4",
		"lodash.debounce": "^4.0.8",
		"lodash.isequal": "^4.5.0",
		"material-icons": "^1.10.4",
		"md5": "^2.3.0",
		"ngx-color": "^7.3.3",
		"nw": "^0.70.0",
		"pako": "^2.0.4",
		"percent-round": "^2.2.1",
		"redux": "^4.0.5",
		"remark": "^13.0.0",
		"remark-html": "^13.0.1",
		"remark-loader": "^4.0.0",
		"rxjs": "^7.5.1",
		"shelljs": "^0.8.4",
		"three": "^0.126.1",
		"three-orbit-controls": "^82.1.0",
		"typescript-json-schema": "0.55.0",
		"zone.js": "^0.11.4"
	},
