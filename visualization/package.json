{
	"author": "MaibornWolff GmbH <dist_intern_codecharta@maibornwolff.de>",
	"homepage": "https://maibornwolff.github.io/codecharta/",
	"bugs": {
		"url": "https://github.com/MaibornWolff/codecharta/issues",
		"email": "dist_intern_codecharta@maibornwolff.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MaibornWolff/codecharta.git"
	},
	"name": "codecharta-visualization",
	"version": "1.74.0",
	"description": "MaibornWolff CodeCharta",
	"files": [
		"dist/webpack",
		"app",
		"conf",
		"Dockerfile",
		"LICENSE.md",
		"package.json",
		"package-lock.json",
		"README.md",
		"tsconfig.json",
		"cli.js"
	],
	"codecharta": {
		"apiVersion": "1.2"
	},
	"window": {
		"icon": "app/icon.png",
		"toolbar": true,
		"frame": true,
		"width": 1024,
		"height": 768
	},
	"main": "dist/webpack/index.html",
	"build": {
		"nwVersion": "0.53.0",
		"output": "./dist/packages/",
		"outputPattern": "codecharta-visualization-${VERSION}-${PLATFORM}-${ARCH}",
		"files": "./dist/webpack/**/*",
		"packed": true,
		"targets": [
			"zip"
		],
		"appId": "io.github.maibornwolff.codecharta",
		"win": {
			"productName": "codecharta-visualization",
			"companyName": "MaibornWolff GmbH",
			"icon": "app/assets/icon.ico"
		},
		"mac": {
			"icon": "app/assets/icon.icns",
			"displayName": "codecharta-visualization"
		}
	},
	"browserslist": [
		"last 2 versions",
		"not IE 11"
	],
	"scripts": {
		"start": "nw .",
		"build": "webpack --config ./conf/webpack.config.js --env STANDALONE=true --env DEV=false",
		"build:web": "webpack --config ./conf/webpack.config.js --env STANDALONE=false --env DEV=false",
		"dev": "webpack serve --config ./conf/webpack.config.js --env STANDALONE=false --env DEV=true",
		"test": "jest --config ./jest.config.json .*\\.spec\\.ts --env=jsdom",
		"test:auto": "npm test -- --watch",
		"test:autoNoCoverage": "npm run test:auto -- --coverage false",
		"test:updateSnaps": "npm test -- --updateSnapshot",
		"e2e": "jest --config ./jest.config.json .*\\.e2e\\.ts --coverage false",
		"e2e:auto": "npm run e2e -- --watch",
		"e2e:ci": "npm run e2e -- --runInBand",
		"package": "rimraf dist/packages && build --concurrent --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ . && bestzip ./dist/packages/codecharta-visualization-$npm_package_version-web.zip ./dist/webpack",
		"plop": "tsc --esModuleInterop true --outDir ./dist/plop ./plopfile.ts && cd dist/plop && plop",
		"lint": "eslint \"app/**/*.ts\"",
		"lint:fix": "npm run lint -- --fix",
		"precommit": "lint-staged",
		"schema:generate": "typescript-json-schema ./app/codeCharta/codeCharta.model.ts ExportCCFile --required --topRef --out app/codeCharta/util/generatedSchema.json"
	},
	"lint-staged": {
		"app/**/*.ts": "eslint --fix"
	},
	"bin": {
		"codecharta-visualization": "cli.js"
	},
	"license": "BSD-3-Clause",
	"dependencies": {
		"@types/webpack": "^5.28.0",
		"ajv": "^6.12.6",
		"angular": "^1.8.2",
		"angular-animate": "^1.8.2",
		"angular-aria": "^1.8.2",
		"angular-material": "^1.2.2",
		"angular-mocks": "^1.8.2",
		"angular-sanitize": "^1.8.2",
		"angularjs-nouislider": "^14.1.3",
		"angularjs-slider": "^7.0.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"browserify-zlib": "^0.2.0",
		"color-convert": "^2.0.1",
		"core-js": "^3.10.1",
		"d3-hierarchy": "^2.0.0",
		"font-awesome": "^4.7.0",
		"ignore": "^5.1.8",
		"jquery": "^3.6.0",
		"klona": "^2.0.4",
		"lodash.camelcase": "^4.3.0",
		"lodash.debounce": "^4.0.8",
		"lodash.isequal": "^4.5.0",
		"lodash.throttle": "^4.1.1",
		"md5": "^2.3.0",
		"nouislider": "^15.0.0",
		"nw": "^0.53.0",
		"open": "^8.0.6",
		"redux": "^4.0.5",
		"shelljs": "^0.8.4",
		"three": "^0.126.1",
		"three-orbit-controls": "^82.1.0",
		"typescript-json-schema": "^0.50.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-typescript": "^7.13.0",
		"@types/angular": "^1.8.1",
		"@types/angular-mocks": "^1.7.0",
		"@types/color-convert": "^2.0.0",
		"@types/d3-hierarchy": "^2.0.0",
		"@types/expect-puppeteer": "^4.4.5",
		"@types/jest": "^26.0.22",
		"@types/jest-environment-puppeteer": "^4.4.1",
		"@types/lodash": "^4.14.168",
		"@types/node": "^15.0.0",
		"@types/puppeteer": "^5.4.3",
		"@types/three": "^0.126.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"angularjs-color-picker": "^3.4.8",
		"babel-loader": "^8.2.2",
		"bestzip": "^2.2.0",
		"canvas": "^2.7.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^5.2.1",
		"es6-shim": "^0.35.6",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-unicorn": "^31.0.0",
		"file-loader": "^6.2.0",
		"html-loader": "^1.1.0",
		"html-loader-jest": "^0.2.1",
		"html-webpack-plugin": "^5.3.1",
		"jest": "^26.6.3",
		"jest-puppeteer": "^5.0.1",
		"jest-serializer-html": "^7.0.0",
		"lint-staged": "^11.0.0",
		"makeshift": "^1.1.0",
		"node-polyfill-webpack-plugin": "^1.1.0",
		"node-sass": "^5.0.0",
		"nwjs-builder-phoenix": "^1.15.0",
		"plop": "^2.7.4",
		"puppeteer": "^8.0.0",
		"rimraf": "^3.0.2",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-jest": "^26.5.5",
		"ts-loader": "^9.0.0",
		"typescript": "^4.2.4",
		"webpack": "^5.33.2",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-glsl-loader": "^1.0.1"
	},
	"optionalDependencies": {
		"7zip-bin-linux": "^1.3.1",
		"7zip-bin-mac": "^1.0.1",
		"7zip-bin-win": "^2.2.0"
	}
}
