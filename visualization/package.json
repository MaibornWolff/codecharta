{
  "author": "MaibornWolff GmbH <dist_intern_codecharta@maibornwolff.de>",
  "homepage": "https://maibornwolff.github.io/codecharta/",
  "bugs": {
    "url": "https://github.com/MaibornWolff/codecharta/issuess",
    "email": "dist_intern_codecharta@maibornwolff.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaibornWolff/codecharta.git"
  },
  "name": "codecharta-visualization",
  "version": "1.6.5",
  "description": "MaibornWolff CodeCharta",
  "files": [
    "dist/webpack",
    "app",
    "conf",
    "mocks",
    "script",
    ".babelrc",
    "Dockerfile",
    "Gruntfile.js",
    "LICENSE.md",
    "package.json",
    "package-lock.json",
    "README.md",
    "tsconfig.json",
    "tsconfig.webpack.json",
    "cli.js"
  ],
  "window": {
    "icon": "app/icon.png",
    "toolbar": true,
    "frame": true,
    "width": 1024,
    "height": 768
  },
  "main": "dist/webpack/index.html",
  "build": {
    "nwVersion": "0.27.3",
    "output": "./dist/packages",
    "files": "./dist/webpack/**/*",
    "packed": true,
    "targets": [
      "zip"
    ],
    "appId": "io.github.maibornwolff.codecharta",
    "win": {
      "companyName": "MaibornWolff GmbH",
      "icon": "app/icon.ico"
    },
    "mac": {
      "icon": "app/icon.icns"
    }
  },
  "scripts": {
    "start": "run --mirror https://dl.nwjs.io/ .",
    "build": "grunt build",
    "test": "jest --runInBand",
    "test:auto": "jest --watch",
    "test:updateSnaps": "jest --updateSnapshot",
    "doc": "grunt doc",
    "serve": "grunt serve",
    "package": "grunt package",
    "prepare": "npm run build"
  },
  "bin": {
    "codecharta-visualization": "cli.js"
  },
  "license": "BSD-3-Clause",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/dist/coverage",
    "coveragePathIgnorePatterns": [
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
      "<rootDir>/app/ng.mockhelper.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/styleMock.js",
      "\\.(html)$": "<rootDir>/mocks/htmlMock.js"
    },
    "roots": [
      "<rootDir>/app"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "skipBabel": false
      }
    }
  },
  "dependencies": {
    "nwjs-builder-phoenix": "^1.14.6",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "makeshift": "^1.1.0",
    "@types/angular": "^1.6.32",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-compress": "^1.4.1",
    "grunt-exec": "1.0.1",
    "grunt-force-task": "^2.0.0",
    "grunt-nw-builder": "^3.1.0",
    "grunt-typedoc": "^0.2.4",
    "grunt-webpack": "^3.0.2",
    "load-grunt-tasks": "^3.5.2",
    "@types/angular-mocks": "^1.5.11",
    "@types/d3": "^4.12.0",
    "@types/d3-hierarchy": "^1.1.0",
    "@types/jest": "^21.1.4",
    "@types/node": "^8.0.46",
    "@types/sinon": "^2.3.5",
    "@types/three": "^0.84.27",
    "ajv": "^5.2.2",
    "angular": "^1.6.0",
    "angularjs-slider": "^6.2.2",
    "d3": "^4.4.0",
    "file-api": "^0.10.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "materialize-css": "^0.98.0",
    "three": "^0.84.0",
    "three-orbit-controls": "^82.1.0",
    "three-text2d": "^0.3.4",
    "angular-mocks": "^1.6.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.5",
    "deepcopy": "^0.6.3",
    "es6-shim": "^0.35.3",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "ng-annotate-loader": "^0.6.1",
    "path": "^0.12.7",
    "script-loader": "^0.7.0",
    "sinon": "^4.0.1",
    "style-loader": "^0.18.2",
    "time-grunt": "^1.4.0",
    "ts-jest": "^21.1.3",
    "ts-loader": "^2.3.6",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-glsl-loader": "^1.0.1"
  },
  "optionalDependencies": {
    "7zip-bin-mac": "^1.0.1",
    "7zip-bin-win": "^2.1.1",
    "7zip-bin-linux": "^1.2.0"
  }
}
