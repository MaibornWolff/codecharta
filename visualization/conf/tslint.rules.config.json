{
    "rules": {
        "quotemark": [true, "double", "avoid-escape"],
        "curly": true,
        "no-magic-numbers": [true, -1, 0, 1, 2, 0.5],
        "promise-function-async": true,
        "await-promise": true,
        "no-floating-promises": true,
        "no-arg": true,
        "no-eval": true,
        "no-empty": true,
        "no-invalid-this": true,
        "no-return-await": true,
        "no-var-keyword": true,
        "triple-equals": true,
        "class-name": true,
        "semicolon": true
    }
}