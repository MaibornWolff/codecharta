{
	"collectCoverage": true,
	"coverageDirectory": "<rootDir>/../dist/coverage",
	"collectCoverageFrom": ["<rootDir>/../app/**/*.{ts,tsx}", "!**/node_modules/**"],
	"coveragePathIgnorePatterns": ["(/__tests__/.*|\\.(spec|e2e|po))\\.(ts|tsx)$", "<rootDir>/../app/puppeteer.helper.ts"],
	"transform": {
		"\\.(m?js)$": "babel-jest",
		"\\.(ts|tsx)$": "ts-jest"
	},
	"transformIgnorePatterns": ["/node_modules/(?!(three|ngx-color/fesm2020))"],
	"roots": ["<rootDir>/../app"],
	"testMatch": ["**/*.e2e.ts"],
	"moduleFileExtensions": ["js", "json", "ts", "tsx", "mjs"],
	"moduleDirectories": ["node_modules", "<rootDir>/../app"],
	"globals": {
		"ts-jest": {
			"tsconfig": "tsconfig.json",
			"babelConfig": true
		}
	},
	"preset": "./jestE2E-preset.js",
	"testTimeout": 60000,
	"setupFiles": ["<rootDir>/../mocks/localStorageMock.js"]
}
